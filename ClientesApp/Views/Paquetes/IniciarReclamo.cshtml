
@{
    ViewData["Title"] = "IniciarReclamo";
}

<h2>Iniciar Reclamo</h2>

<h4>Ingrese el numero de referencia de su paquete</h4>
<hr />

@using (Html.BeginForm("IniciarReclamo", "Paquetes"))
{
    <div class="form-group">
        <label>Ingrese aqui el codigo</label>
        <input type="text" name="numReferencia" class="form-control" />
        <br />
    </div>
    <div class="form-group">
        <button type="submit" class="btn btn-default">Verificar Paquete</button>
    </div>

}